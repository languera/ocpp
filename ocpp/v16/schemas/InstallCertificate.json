{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "comment": "OCPP 2.0 - v1p0",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "certificateType": {
      "type": "string",
      "additionalProperties": false,
      "enum": [
        "CentralSystemRootCertificate",
        "ManufacturerRootCertificate"
      ]
    },
    "certificate": {
      "type": "string",
      "maxLength": 5500
    }
  },
  "required": [
    "certificateType",
    "certificate"
  ]
}